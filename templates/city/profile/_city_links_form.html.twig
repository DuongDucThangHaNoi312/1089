<div class="col-md-12 ajax-form">
    {{ form_start(form) }}
    <label>City Links</label>
    <div class="collection urls" data-prototype="{{ form_widget(form.urls.vars.prototype)|e('html_attr') }}">
        {# iterate over each existing tag and render its only field: name #}
        {% for url in form.urls %}
            <li class="form-row">
                {{ form_row(url.type) }}
                {{ form_row(url.value) }}
            </li>
        {% endfor %}
    </div>
    {{ form_row(form._token) }}
    <button type="button" class="btn btn-light cancel-btn">Cancel</button>
    <button type="submit" class="btn btn-primary save-btn">Save</button>
    {{ form_end(form, {'render_rest': false}) }}
</div>