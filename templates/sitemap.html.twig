    {% for job in jobAnnouncements %}
        <url>
            <loc>{{ system_domain }}/city/{{ job.jobTitle.city.slug }}/job/announcements/{{ job.id }}/full-view</loc>
            <lastmod>{{ job.updatedAt|date('Y-m-d\\TH:i:s\\+00:00') }}</lastmod>
            <priority>0.22</priority>
        </url>
    {% endfor %}

    {% for county in counties %}
        <url>
            <loc>{{ system_domain }}/job/search/{{ county.state.slug }}/{{ county.slug }}</loc>
            <lastmod>{{lastmod}}</lastmod>
            <priority>0.26</priority>
        </url>
    {% endfor %}

    {% for city in cities %}
        <url>
            <loc>{{ system_domain }}/job/search/{{ city.stateSlug }}/{{ city.countySlug }}/{{ city.citySlug }}</loc>
            <lastmod>{{ lastmod }}</lastmod>
            <priority>0.26</priority>
        </url>
    {% endfor %}
