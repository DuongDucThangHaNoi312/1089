<div class="row ajax-form">
    <div class="col-md-12">
        {{ form_start(form) }}
        <div class="row">
            <div class="col-md-12">
                <div>
                    {% form_theme form 'job_seeker/resume/work_history_theme.html.twig' %}
                        {{ form_widget(form.workHistories) }}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                {{ form_row(form._token) }}
                <button type="button" class="btn btn-light cancel-btn">Cancel</button>
                <button type="submit" class="btn btn-primary save-btn">Save</button>
            </div>
        </div>
        {{ form_end(form, {'render_rest': false}) }}
    </div>
</div>